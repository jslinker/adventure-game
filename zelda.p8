pico-8 cartridge // http://www.pico-8.com
version 15
__lua__
d = 8
x = 114
y = 60
camx = 90
camy = 60
s = .125 // multiple of 8 divided by 1
s2 = s * 6
frameflip = false
idle = {first = 64, last = 66, id = 1}
walk = {first = 80, last = 83, id = 2}
lay = {first = 96, last=96, id = 3}
idleup = {first=120,last=122,id=4}
walkup = {first=117,last=120,id=5}
hold = {first=97,last=97,id=6}
frames = lay
frame = 0
doors = {
	{x=4, y=62, dest=3, id=2, dx = 0, dy = -4},
	{x=104, y=58, dest=2, id=3, dx = 0, dy = 4}
}
items = {
	{x=109, y=61, id=1, tile=7, sprite=114},
	{x=94, y=60, id=2, tile=7, sprite=113}
}
actions = {}
first = true
music(0)
function _update()
	if first then
		first = false
	end
	if #actions > 0 then
		return
	end

	tx = 0
	ty = 0
	if (btn(0)) tx = -s
  	if (btn(1)) tx = s
	if (btn(2)) ty = -s
	if (btn(3)) ty = s
	if tx != 0 or ty > 0 then
		new = walk
	elseif ty != 0 then
		new = walkup
	elseif frames.id != 3 and frames.id != 4 then
		if frames.id == 5 then
			printh(frames.id)
			new = idleup
		else
			new = idle
		end
	else
		new = frames
	end

	if new.id != frames.id then
		frame = 0
	end
	frames = new

	tx = (x + tx)
	ty = (y + ty)
	
	// +/- s constricts the bb
	-- ff = fget(mget(flr(tx + s), flr(ty + s)))
	-- fc = fget(mget(flr(tx + s), ceil(ty - s)))
	-- cf = fget(mget(ceil(tx - s), flr(ty + s)))
	-- cc = fget(mget(ceil(tx - s), ceil(ty - s)))
	
	-- right
	cf = fget(mget(ceil(tx - s), flr(y + s2)))
	cc = fget(mget(ceil(tx - s), ceil(y + s)))
	if tx > x and cf != 1 and cc != 1 then
		x = tx
		frameflip = true
	end

	-- left
	ff = fget(mget(flr(tx + s), flr(y + s2)))
	fc = fget(mget(flr(tx + s), ceil(y + s)))
	if tx < x and ff != 1 and fc != 1 then
		x = tx
		frameflip = false
	end

	-- up
	ff = fget(mget(flr(x + s), flr(ty + s2)))
	cf = fget(mget(ceil(x - s), flr(ty + s2)))
	if ty < y and cf != 1 and ff != 1 then
		y = ty
	end

	-- down
	fc = fget(mget(flr(x + s), ceil(ty + s)))
	cc = fget(mget(ceil(x - s), ceil(ty + s)))
	if ty > y and cc != 1 and fc != 1 then
		y = ty
	end

	checkdoors()
	checkitems()

	movecamera()
end

function _draw()
		cls()
		map(0,0,0,0,128,64)
		spr(112, (x * d), (y * d) + 1)
		spr(flr(frames.first + frame),x * d,y * d, 1, 1, frameflip)
		frame += 0.2
		if frames.first + frame >= frames.last + 1 then frame = 0 end

		doactions()
end

function doactions()
	action = actions[1]
	if action == nil then
		return
	end
	del(actions, actions[1])

	if action.type == "f" then
		rectfill(camx, camy, camx + 127, camy + 127 - action.percent, 1)
	end
	if action.type == "dm" then
		rectfill(camx, camy, camx + 127, camy + 127, 1)
		x = action.x
		y = action.y
		movecamera()
	end
	if action.type == "i" then
		spr(action.sprite, (x * d), ((y - .8) * d))
	end
end

function checkitems()
	for item in all(items) do
		onitem = abs(item.x - x) <= (1-s) and abs(y - item.y) <= (1-s)
		if onitem then
			mset(item.x, item.y, item.tile)
			frames = hold
			for i=0, 60, 1 do
				wait = {percent=i, type="i", sprite=item.sprite}
				add(actions,wait)
			end
			del(items, item)
			break
		end
	end
end

function checkdoors()
	for door in all(doors) do
		indoor = abs(door.x - x) <= s and abs(y - door.y) <= 0
		if indoor then
			for door2 in all(doors) do
				if door2.id == door.dest then
					newx = door2.x + (door2.dx * s)
					newy = door2.y + (door2.dy * s)

					for i=110, 0, -5 do
						fade = {percent=i, type="f"}
						add(actions,fade)
					end
					add(actions, {type="dm", x=newx, y=newy})
					for i = 0, 130, 5 do
						fade = {percent=i, type="f"}
						add(actions, fade)
					end 

					break
				end
			end
			break
		end
	end
end

function movecamera()
	// aligned 8 px above center
	//(bottom of character 
	// is centered)
	camx = max(0,min(x * d - 56, 897))--x * d - 56
	camy = max(0,min(y * d - 56,385))--y * d - 56
	camera(camx, camy)
	-- camera(max(0,min(camx, 897)), max(0,min(camy,385)))
end
